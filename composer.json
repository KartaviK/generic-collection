{
    "name": "kartavik/generic-collection-php",
    "description": "Realization of generic collection",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "KartaviK",
            "email": "varkuta.roma@gmail.com"
        }
    ],
    "keywords": [
        "designer",
        "collection",
        "generic"
    ],
    "minimum-stability": "stable",
    "require": {},
    "autoload": {
        "psr-4": {
            "kartavik\\Designer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kartavik\\Designer\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
        "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
        "test": "./vendor/bin/phpunit"
    }
}
